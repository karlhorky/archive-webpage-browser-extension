{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "NodeNext",
    "moduleResolution": "Node16",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "checkJs": true
  },
  "include": [
    "eslint.config.js",
    "manifest.config.ts",
    "package.json",
    "prettier.config.mjs",
    "vite.config.ts"
  ]
}
